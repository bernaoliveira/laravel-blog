{
    "preset": "laravel",
    "rules": {
        "simplified_null_return": true,
        "braces": false,
        "new_with_braces": {
            "anonymous_class": false,
            "named_class": false
        },
        "single_class_element_per_statement": false,
        "class_attributes_separation": {
            "elements": ["property"]
        }
    }
}
